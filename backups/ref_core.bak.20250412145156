# -*- coding: utf-8 -*- 
"""Ref核心模块""" 
 
import os 
import sys 
import logging 
 
# 设置日志 
logging.basicConfig( 
    level=logging.INFO, 
    format='%(asctime)s - %(name)s - %(levelname)s - %(message)s' 
) 
 
logger = logging.getLogger('Ref') 
 
class RefCore: 
    """Ref核心类""" 
    def __init__(self): 
        self.initialized = True 
        logger.info('RefCore初始化完成') 
 
    def monitor_file(self, file_path): 
        """监控文件""" 
        if os.path.exists(file_path): 
            logger.info(f'监控文件: {file_path}') 
            return True 
        return False 
 
