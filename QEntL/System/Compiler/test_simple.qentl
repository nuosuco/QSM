// 简单QEntL测试程序
// 用于编译器开发测试

配置 {
    测试模式: true,
    版本: "1.0.0"
}

类型 用户 {
    ID: 字符串,
    名称: 字符串,
    年龄: 整数
}

函数 问候(名称: 字符串) -> 字符串 {
    如果 名称 == "" {
        返回 "你好，未知用户！"
    } 否则 {
        返回 "你好，" + 名称 + "！"
    }
}

// 主程序
quantum_program 测试程序 {
    setup: 函数() {
        日志("初始化测试程序...")
        let 用户1 = { ID: "001", 名称: "张三", 年龄: 30 }
        let 问候语 = 问候(用户1.名称)
        日志(问候语)
    },
    
    run: 函数() {
        循环 i 在 [1, 2, 3, 4, 5] {
            日志("循环迭代: " + i)
        }
    }
}